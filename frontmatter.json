{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "header",
          "name": "header",
          "type": "fields",
          "fields": [
            {
              "title": "overlay_image",
              "name": "overlay_image",
              "type": "image"
            },
            {
              "title": "overlay_filter",
              "name": "overlay_filter",
              "type": "datetime"
            },
            {
              "title": "teaser",
              "name": "teaser",
              "type": "string"
            }
          ]
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "sidebar",
          "name": "sidebar",
          "type": "fields",
          "fields": [
            {
              "title": "nav",
              "name": "nav",
              "type": "string"
            }
          ]
        },
        {
          "title": "toc",
          "name": "toc",
          "type": "boolean"
        },
        {
          "title": "toc_sticky",
          "name": "toc_sticky",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "path": "[[workspace]]/_posts/Algo_BOJ"
    }
  ]
}